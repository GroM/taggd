{
	"extends": "airbnb",
	"env": {
		"jasmine": true,
		"browser": true,
		"jquery": true
	},
	"rules": {
		"indent": ["error", "tab"],
		"brace-style": ["error", "allman"],
		"no-tabs": ["error", {"allowIndentationTabs": true}],
		"arrow-parens": ["error", "always"],
		"keyword-spacing": ["error", {
			"overrides":
			{
				"if": { "after": false },
				"for": { "after": false },
				"while": { "after": false }
			}
		}]
	}
}
